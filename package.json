{
    "name": "ts-template-frontend",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start:prod": "ng serve --port=4281 --host=0.0.0.0",
        "start": "ng serve --port=4281 --host=0.0.0.0 --disable-host-check -c development",
        "build": "ng version && ng build -c production",
        "build:test": "ng version; ng build -c test",
        "lint": "eslint . --ext .ts",
        "lint:json": "eslint . --ext .ts -f json -o .eslint-results.json",
        "lint:fix": "eslint . --fix --ext .ts",
        "sonar": "sonar-scanner",
        "translations": "ngx-translate-extract --input ./src --output ./src/assets/i18n/nl-NL.json ./src/assets/i18n/en-GB.json --clean --sort --format namespaced-json --marker translationMarker",
        "translations:add": "git add ./src/assets/i18n",
        "translations:check": "node ./checkTranslations.js"
    },
    "private": true,
    "pre-commit": {
        "run": [
            "lint:fix",
            "translations",
            "translations:check",
            "translations:add"
        ]
    },
    "dependencies": {
        "@angular/animations": "15.0.2",
        "@angular/cdk": "15.0.1",
        "@angular/common": "15.0.2",
        "@angular/compiler": "15.0.2",
        "@angular/core": "15.0.2",
        "@angular/forms": "15.0.2",
        "@angular/platform-browser": "15.0.2",
        "@angular/platform-browser-dynamic": "15.0.2",
        "@angular/router": "15.0.2",
        "@bartholomej/ngx-translate-extract": "8.0.2",
        "@biesbjerg/ngx-translate-extract-marker": "1.0.0",
        "@material/dialog": "14.0.0",
        "@ngx-translate/core": "14.0.0",
        "@ngx-translate/http-loader": "7.0.0",
        "@ngxs/devtools-plugin": "3.7.6",
        "@ngxs/router-plugin": "3.7.6",
        "@ngxs/storage-plugin": "3.7.6",
        "@ngxs/store": "3.7.6",
        "@typescript-eslint/eslint-plugin": "5.11.0",
        "@typescript-eslint/parser": "5.11.0",
        "angular-svg-icon": "13.0.0",
        "bootstrap": "5.2.3",
        "crypto-browserify": "3.12.0",
        "crypto-js": "4.1.1",
        "http-browserify": "1.7.0",
        "https-browserify": "1.0.0",
        "jwt-decode": "3.1.2",
        "lottie-web": "5.10.0",
        "moment": "2.29.4",
        "ng-click-outside": "9.0.1",
        "ngx-bootstrap": "9.0.0",
        "ngx-color-picker": "13.0.0",
        "ngx-device-detector": "4.0.1",
        "ngx-lottie": "10.0.0",
        "ngx-popper": "7.0.0",
        "ngx-toastr": "16.0.1",
        "node-pre-gyp": "0.17.0",
        "os-browserify": "0.3.0",
        "popper.js": "1.16.1",
        "rxjs": "7.5.4",
        "sass-rem": "3.0.0",
        "sonar-scanner": "3.1.0",
        "stream-browserify": "3.0.0",
        "tslib": "2.4.1",
        "web3": "1.7.5",
        "zone.js": "0.12.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "15.0.2",
        "@angular/cli": "15.0.2",
        "@angular/compiler-cli": "15.0.2",
        "@types/node": "18.11.10",
        "@typescript-eslint/eslint-plugin": "5.36.1",
        "@typescript-eslint/parser": "5.36.1",
        "eslint": "8.23.0",
        "eslint-plugin-no-loops": "0.3.0",
        "pre-commit": "1.2.2",
        "typescript": "4.8.4"
    },
    "browser": {
        "fs": false,
        "zlib": false,
        "assert": false,
        "tls": false,
        "url": false,
        "net": false,
        "path": false
    }
}
